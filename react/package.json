{
  "name": "ccm-stager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-table/core": "^4.3.44",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@types/node": "^16.11.27",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.1",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "notistack": "^1.0.10",
    "prettier": "2.6.2",
    "re-resizable": "^6.9.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-file-drop": "^3.1.3",
    "react-query": "^3.35.0",
    "react-router-dom": "^5.3.1",
    "react-scripts": "5.0.1",
    "typescript": "~4.6.3"
  },
  "resolutions": {
    "@types/eslint": "^8.4.1",
    "@types/estree": "^0.0.51",
    "@types/node": "^16.11.27",
    "@types/react": "^17.0.44"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "fix-lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "check-format": "prettier --check src",
    "format": "prettier --write src"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "import",
      "mui-unused-classes"
    ],
    "rules": {
      "import/no-useless-path-segments": "warn",
      "import/no-cycle": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            [
              "parent",
              "sibling",
              "index"
            ]
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ],
      "mui-unused-classes/unused-classes": "warn",
      "react/jsx-curly-brace-presence": "warn",
      "react/jsx-key": "warn",
      "sort-imports": [
        "warn",
        {
          "ignoreDeclarationSort": true,
          "ignoreCase": true
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "arrowParens": "avoid"
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "production": [
      ">0.2% in CA",
      "not dead",
      "not op_mini all",
      "not ie >0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
