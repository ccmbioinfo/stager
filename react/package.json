{
  "name": "ccm-stager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-table/core": "^4.3.5",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "notistack": "^1.0.10",
    "prettier": "2.5.0",
    "re-resizable": "^6.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-file-drop": "^3.0.7",
    "react-query": "^3.34.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "~4.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "fix-lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "check-format": "prettier --check src",
    "format": "prettier --write src"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "import",
      "mui-unused-classes"
    ],
    "rules": {
      "import/no-useless-path-segments": "warn",
      "import/no-cycle": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            [
              "parent",
              "sibling",
              "index"
            ]
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react"
          ],
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ],
      "mui-unused-classes/unused-classes": "warn",
      "react/jsx-curly-brace-presence": "warn",
      "react/jsx-key": "warn",
      "sort-imports": [
        "warn",
        {
          "ignoreDeclarationSort": true,
          "ignoreCase": true
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "arrowParens": "avoid"
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "production": [
      ">0.2% in CA",
      "not dead",
      "not op_mini all",
      "not ie >0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
